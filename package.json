    },
    "author": "bert-w",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/bert-w/sqomplexity.git"
    },
    "devDependencies": {
        "babel-loader": "^9.1.2",
        "chai": "^4.3.7",
        "mocha": "^10.2.0",
        "pegjs": "^0.10.0",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "commander": "^11.0.0",
        "lodash.get": "^4.4.2"
    },
    "engines": {
        "node": ">=16"
